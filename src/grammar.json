{
  "name": "snap",
  "rules": {
    "source_file": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "header"
        },
        {
          "type": "SYMBOL",
          "name": "body"
        }
      ]
    },
    "header": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "---"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "TOKEN",
              "content": {
                "type": "PATTERN",
                "value": ".*"
              }
            },
            {
              "type": "STRING",
              "value": "---"
            }
          ]
        }
      ]
    },
    "body": {
      "type": "PATTERN",
      "value": "(.|[\\r\\n])*"
    },
    "metadata": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "FIELD",
          "name": "value",
          "content": {
            "type": "SYMBOL",
            "name": "_value"
          }
        }
      ]
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[a-z_0-9]+"
    },
    "_value": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "string"
        }
      ]
    },
    "string": {
      "type": "CHOICE",
      "members": [
        {
          "type": "PATTERN",
          "value": "\\\"[^\\\"]*\\\""
        },
        {
          "type": "PATTERN",
          "value": "[^\\s]+"
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

