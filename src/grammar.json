{
  "name": "snap",
  "rules": {
    "source_file": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "header",
          "content": {
            "type": "SYMBOL",
            "name": "header"
          }
        },
        {
          "type": "FIELD",
          "name": "body",
          "content": {
            "type": "SYMBOL",
            "name": "body"
          }
        }
      ]
    },
    "header": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "sep"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "metadata"
          }
        },
        {
          "type": "SYMBOL",
          "name": "sep"
        }
      ]
    },
    "sep": {
      "type": "STRING",
      "value": "---"
    },
    "metadata": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "FIELD",
          "name": "value",
          "content": {
            "type": "SYMBOL",
            "name": "_value"
          }
        }
      ]
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[a-z_0-9]+"
    },
    "_value": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "string"
        },
        {
          "type": "SYMBOL",
          "name": "path"
        }
      ]
    },
    "string": {
      "type": "PATTERN",
      "value": "\\\"[^\\\"]*\\\""
    },
    "path": {
      "type": "PATTERN",
      "value": "\\/?[^\\/\\s]+(?:\\/[^\\/\\s]+)*"
    },
    "body": {
      "type": "PATTERN",
      "value": "(.|[\\r\\n])*"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

